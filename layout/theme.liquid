<!doctype html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">

    {% if settings.favicon_light %}
      <link rel="icon" href="{{ settings.favicon_light | img_url: '32x32' }}" type="image/png" id="favicon-light">
    {% endif %}
    {% if settings.favicon_dark %}
      <link rel="icon" href="{{ settings.favicon_dark | img_url: '32x32' }}" type="image/png" id="favicon-dark">
    {% endif %}

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">    
    <link rel="preload" href="{{ 'air_purifier.gltf' | asset_url }}" as="fetch" type="model/gltf-binary" crossorigin="anonymous">

    
    {% style %}
      @font-face {
          font-family: "General Sans Regular";
          src: url( {{ 'GeneralSans-Regular.otf' | asset_url | append: '?v=' | append: 'timestamp' }} );
      }
      @font-face {
          font-family: "General Sans Bold";
          src: url( {{ 'GeneralSans-Bold.otf' | asset_url | append: '?v=' | append: 'timestamp' }} );
      }
      @font-face {
          font-family: "General Sans Medium";
          src: url( {{ 'GeneralSans-Medium.otf' | asset_url | append: '?v=' | append: 'timestamp' }} );
      }
      @font-face {
          font-family: "General Sans Semibold";
          src: url( {{ 'GeneralSans-Semibold.otf' | asset_url | append: '?v=' | append: 'timestamp' }} );
      }
      @font-face {
          font-family: "Avenir";
          src: url( {{'avenir.ttf' | asset_url | append: '?v=' | append: 'timestamp'}} );
      }
    {% endstyle %}

    {{ 'index.css' | asset_url | stylesheet_tag }}

    {{ content_for_header }}
    

    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />

    <script src="https://cdn.jsdelivr.net/npm/gsap@3.12.7/dist/gsap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/gsap@3.12.7/dist/ScrollTrigger.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.0/jquery.min.js" integrity="sha512-3gJwYpMe3QewGELv8k/BX9vcqhryRdzRMxVfq6ngyWXwo03GFEzjsUm8Q7RZcHPHksttq7/GFoxjCVUjkjvPdw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/three@0.128/examples/js/loaders/GLTFLoader.js"></script>

    <script>
      document.addEventListener("DOMContentLoaded", function() {
        var lightFavicon = document.getElementById("favicon-light");
        var darkFavicon = document.getElementById("favicon-dark");
  
        if (window.matchMedia && window.matchMedia("(prefers-color-scheme: dark)").matches) {
          
          if (darkFavicon) {
            darkFavicon.rel = "icon";
            lightFavicon.rel = "icon alternate";
          }
        } else {
          if (lightFavicon) {
            lightFavicon.rel = "icon";
            darkFavicon.rel = "icon alternate";
          }
        }
      });
    </script>
    
  </head>
  <body>

    {% if template != '404' %}
      {% if template == 'index' %}
        <nav class="black home">
          {% section 'navbar' %}
        </nav>
      {% else %}
        <nav class="black">
          {% section 'navbar' %}
        </nav>
      {% endif %}
    {% endif %}
    
    <main role="main">
      {{ content_for_layout }}
    </main>
    
    {% if template != '404' %}
      {% section 'footer' %}
    {% endif %}


    <script async src="https://static.klaviyo.com/onsite/js/klaviyo.js?company_id=UkKEks"></script>

    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

    {% comment %} {{ 'index.js' | asset_url | script_tag }} {% endcomment %}
  </body>
</html>